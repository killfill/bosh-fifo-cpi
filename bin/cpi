#!/usr/bin/env node

var fifoCpi = require('../index')

var input = ''
process.stdin.on('data', function(d) {input+= d})
process.stdin.on('end', function() {

	input = JSON.parse(input)

	fifoCpi
		.method(input.method)
		.withArguments(input.arguments)
		.end(function result(res) {
			console.log(JSON.stringify(res))
		})

})